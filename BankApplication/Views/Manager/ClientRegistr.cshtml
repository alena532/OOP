@using Microsoft.AspNetCore.Identity;
@model List<UserCreation>
    <h1>Reguests</h1>
<table>
    <tr>
        <th>UserName</th>
        <th>Email</th>
    </tr>


            
@foreach(var user in @Model)
{
    <tr>
        <td>@user.UserName</td>
        <td>@user.Email</td>
        <td>
            <form asp-action="Reject" asp-route-userName='@user.Id' method="post">
                    
                <button type="submit"> Reject</button>
            </form>

            <form asp-action="Confirm" asp-route-userName='@user.Id' method="post">
                    
                   <button type="submit"> Confirm</button>
            </form>
           
         </td>
         </tr>
}
            
    
 </table>
